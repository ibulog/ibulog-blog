---
export interface Props {
	class?: string;
}

const { class: className } = Astro.props;
---

<svg
	width="24"
	height="100%"
	viewBox="0 0 24 10"
	preserveAspectRatio="none"
	xmlns="http://www.w3.org/2000/svg"
	class={className || "h-full w-4"}
>
	<line
		x1="1"
		y1="0"
		x2="1"
		y2="10"
		stroke="var(--color-app-border-default)"
		stroke-width="2"
	/>
	<line
		x1="9"
		y1="0"
		x2="9"
		y2="10"
		stroke="var(--color-app-border-default)"
		stroke-width="2"
	/>
	<line
		x1="17"
		y1="0"
		x2="17"
		y2="10"
		stroke="var(--color-app-border-default)"
		stroke-width="2"
	/>
</svg>
